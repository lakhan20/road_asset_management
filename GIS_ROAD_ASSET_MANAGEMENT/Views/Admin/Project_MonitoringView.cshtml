    <script>
        // JavaScript code
        // Initialize DataTable
        $(document).ready(function () {
            new DataTable('#example', {
                searchPanes: true,
                dom: 'Plfrtip',
                language: {
                    searchPanes: {
                        emptyPanes: ''
                    }
                }
            });

            // Event handler for opening the first modal
            $('#openFirstModal').click(() => {
                $('#exampleModal1').modal('show');
            });

            // Event handler for moving to the second modal
            $('#right1').click(() => {
                $('#exampleModal2').modal('show');
                $('#exampleModal1').modal('hide');
            });

            // Event handler for moving back to the first modal
            $('#left2').click(() => {
                $('#exampleModal1').modal('show');
                $('#exampleModal2').modal('hide');
            });

            // Event handler for moving to the third modal
            $('#right2').click(() => {
                $('#exampleModal3').modal('show');
                $('#exampleModal2').modal('hide');
            });

            // Event handler for moving back to the second modal
            $('#left3').click(() => {
                $('#exampleModal2').modal('show');
                $('#exampleModal3').modal('hide');
            });
        });
    </script>
